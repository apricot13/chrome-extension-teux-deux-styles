{
  "name": "TeuxDeux: Styles",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "adds styles to teux-deux",
  "homepage_url": "https://apricot13.org",
  "host_permissions": [
    "https://teuxdeux.com/home/*",
    "https://teuxdeux.com/home/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://teuxdeux.com/home/*", "https://teuxdeux.com/home/*"],
      "js": ["inject.js"],
      "css": ["inject.css"]
    }
  ]
}
